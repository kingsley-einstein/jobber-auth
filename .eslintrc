{
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "eqeqeq": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "no-param-reassign": "off",
    "global-require": "off",
    "import/no-extraneous-dependencies": "off",
    "no-console": "off"
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "module"
  },
  "parser": "babel-eslint"
}